java.util.* をインポートします。



クラスメイン{

    文字列項目。

    静的クラス 内部{
        静的Main.Inner2 inner = new Main().new Inner2(0);

        public static Main.Inner2 makeLinkedList(int[] a){
            Main.Inner2 ite = 内部;
            for(int num : a) {
                ite.next = 新しい Main()。新しい Inner2(数値);
                ite = ite.next;
            }
            内部を返します。
        }
    }

    パブリッククラス Inner2{
        プライベート int データ。
        次はプライベート Inner2;

        Inner2(int データ){
            this.data = データ;
            this.next = null;
        }
    }


    public static void main(String[] args) throws Exception {
        Main.Inner head = new Main.Inner();
        int[] a = {1,2,3};
        Main.Inner2 newhead = head.makeLinkedList(a);
        Main.Inner2 イテレータ = newhead;
        while(イテレータ != null){
            System.out.println(iterator.data);
            イテレータ = イテレータ.next;
        }
        // System.out.println(head);
    }
}
